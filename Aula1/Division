#Ejercicio3
def array_div(array):
    #La funcion retorna dos array uno de numeros pares
    #y otro de numeros impares
    pair =[]
    odd=[]
    for i in array:
        if i%2==0:
            pair.append(i);
        else:
            odd.append(i)
    return pair,odd

numbers=[1, 3,2, 4, 5, 6, 7]
val= array_div(numbers)
print(val)
